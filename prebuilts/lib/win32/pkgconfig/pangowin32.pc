prefix=/mingw64
libdir=${prefix}/lib
includedir=${prefix}/include

Name: Pango Win32
Description: Win32 GDI font support for Pango
Version: 1.48.7
Requires: pango
Requires.private: glib-2.0 >=  2.62, gobject-2.0 >=  2.62, gio-2.0 >=  2.62, fribidi >=  0.19.7, libthai >=  0.1.9, harfbuzz >=  2.0.0, fontconfig >=  2.11.91, freetype2, cairo >=  1.12.10, harfbuzz-gobject >=  2.0.0
Libs: -L${libdir} -lpangowin32-1.0
Libs.private: -lm -lgdi32
Cflags: -I${includedir}/pango-1.0
