prefix=/clang64
exec_prefix=${prefix}
libdir=/clang64/lib
includedir=/clang64/include

Name: absl_log_sink
Description: Abseil log_sink library
URL: https://abseil.io/
Version: 20230125
Requires: absl_config = 20230125, absl_log_entry = 20230125
Libs: -L${libdir}   -labsl_log_sink
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
