prefix=/clang64
exec_prefix=${prefix}
libdir=/clang64/lib
includedir=/clang64/include

Name: absl_civil_time
Description: Abseil civil_time library
URL: https://abseil.io/
Version: 20230125
Requires:
Libs: -L${libdir}   -labsl_civil_time
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
