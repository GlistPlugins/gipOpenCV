prefix=/clang64
exec_prefix=${prefix}
bindir=${prefix}/bin
libexecdir=${prefix}/share/qt6/bin
libdir=${prefix}/lib
includedir=${prefix}/include/qt6
mkspecsdir=${prefix}/share/qt6/mkspecs

Name: Qt6 Platform
Description: Qt Platform module
Version: 6.5.1
Cflags: -I${mkspecsdir}/win32-clang-g++ -I${includedir} -DWIN32 -D_ENABLE_EXTENDED_ALIGNED_STORAGE -DWIN64 -D_WIN64 -DMINGW_HAS_SECURE_API=1 -D_WIN32_WINNT=0x0A00 -DWINVER=0x0A00
