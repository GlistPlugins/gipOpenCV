prefix=/clang64
exec_prefix=${prefix}
libdir=/clang64/lib
includedir=/clang64/include

Name: absl_random_internal_wide_multiply
Description: Abseil random_internal_wide_multiply library
URL: https://abseil.io/
Version: 20230125
Requires: absl_bits = 20230125, absl_config = 20230125, absl_int128 = 20230125
Libs: -L${libdir}  
Cflags: -I${includedir} -Wno-float-conversion -Wno-implicit-float-conversion -Wno-implicit-int-float-conversion -Wno-unknown-warning-option -DNOMINMAX
